; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "CanvasLayer",
"class": "Colorblindness",
"language": "GDScript",
"path": "res://addons/paulloz.colorblindness/colorblindness.gd"
} ]
_global_script_class_icons={
"Colorblindness": "res://addons/paulloz.colorblindness/colorblindness.svg"
}

[application]

config/name="Last Moment"
run/main_scene="res://UI/Scene/Menu/Menu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="pckrt/Last Moment"
boot_splash/image="res://Objects/Sun/Sun.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.960784, 0.866667, 0.737255, 1 )
config/icon="res://UI/Last Moment Logo Kopie.png"
config/macos_native_icon="res://UI/Last Moment Logo.icns"
config/windows_native_icon="res://UI/Last Moment Logo.ico"

[autoload]

Global="*res://Global/Global.gd"
Tool="*res://Global/Tool.gd"
Time="*res://Global/Time.gd"
Save="*res://Global/Save.gd"
Eco="*res://Global/Eco.gd"

[display]

window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/gdfxr/plugin.cfg", "res://addons/paulloz.colorblindness/plugin.cfg" )

